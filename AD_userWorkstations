#restringe en qué computadoras un usuario puede iniciar sesión.
#Para asignar una o varias estaciones de trabajo a un solo usuario:
Set-ADUser -Identity "jepintocta" -LogonWorkstations "pcsegdatos1,srvcoeepo5"


Get-ADUser -Identity "jepintocta" -Properties userWorkstations



Set-ADUser -Identity "jepintocta" -Clear userWorkstations
